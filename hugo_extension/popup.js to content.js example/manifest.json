{
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
      "manifest_version": 2,
      "name": "Automatic Image Switcher",
      "version": "0.2",
      "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "icons": { "16": "dharma_16.png",
             "48": "dharma_48.png",
            "128": "dharma_128.png" },
    "permissions": [
        "tabs"
      ]
    }